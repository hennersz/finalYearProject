[run]
branch = True
source = p2ppki/
omit = pisces/*
       pisces/pisces/*
       pisces/pisces/spkilib/*

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
omit =
    tests/*
    pisces/pisces/*
    pisces/pisces/spkilib/*
    setup.py
